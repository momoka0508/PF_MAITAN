<div class = "container mt-5">
  <h1 class = "mx-auto">【ブックマーク一覧】</h1>
  <table class="table table-hover table-inverse">
    <thead>
      <tr>
        <th>IT用語</th>
        <th>意味</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <% @bookmarks.each do |bookmark| %>
        <tr>
          <td><%= bookmark.it_word.word %></td>
          <td><%= bookmark.it_word.body %></td>
          <!--ブックマーク-->
          <%= render "bookmarks/bookmark", it_word: bookmark.it_word %>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>